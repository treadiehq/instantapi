{
  "name": "@instantapihq/client",
  "version": "0.1.0",
  "description": "Client SDK for InstantAPI - Run AI agents with an API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "instantapi",
    "ai",
    "agents",
    "api",
    "sdk",
    "openai",
    "anthropic",
    "serverless"
  ],
  "author": "InstantAPI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/treadiehq/instantapi.git",
    "directory": "packages/sdk-client"
  },
  "bugs": {
    "url": "https://github.com/treadiehq/instantapi/issues"
  },
  "homepage": "https://instantapi.co",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}

